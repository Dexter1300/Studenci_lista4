<Window x:Class="Studenci_lista4.Window3"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Studenci_lista4"
        mc:Ignorable="d"
        Title="Window3" Height="230" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Label Grid.Row="0">Imie</Label>
        <Label Grid.Row="1">Nazwisko</Label>
        <Label Grid.Row="2">Wiek</Label>
        <Label Grid.Row="3">Pesel</Label>
        <TextBox Grid.Row="0" Height="25" Width="200" Margin="50,5,10,5" Name="imie" IsEnabled="False"/>
        <TextBox Grid.Row="1" Height="25" Width="200" Margin="50,5,10,5" Name="nazwisko" IsEnabled="False"/>
        <TextBox Grid.Row="2" Height="25" Width="200" Margin="50,5,10,5" Name="wiek" IsEnabled="False"/>
        <TextBox Grid.Row="3" Height="25" Width="200" Margin="50,5,10,5" Name="Pesel" IsEnabled="False"/>
    </Grid>
</Window>
